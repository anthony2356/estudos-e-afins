programa {
  funcao inicio() {
    inteiro valor_recebido, calcular_valor_recebido, resto
    escreva("insira um valor inteiro a ser calculado: \n")
    leia(valor_recebido)
    calcular_valor_recebido = valor_recebido / 3
    resto = 0
    escreva("calcuando...")
    escreva("o quociente é : " + calcular_valor_recebido + "\n")
    escreva(" e o resto é igual a : " + resto)
  }
}
